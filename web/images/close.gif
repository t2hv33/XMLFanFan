            $('#ajax-saving').scrollFollow();
            $('#ajax-loading').scrollFollow();
            $('#ajax-processing').scrollFollow();
            $('#new-widget').click(function(){
                if ($('#widget-list').val()!='') {
                    $.ajax({
                        type: "GET",
                        url: "/layout/EnablePortlet/layout/it-portal-full-content&name="+$('#widget-list').val()+"&col="+ $('#content').children().next().next().attr('id').substr(7, 256),
                        success: function(data) {
                            window.location.replace("/themes/classic/images/close.gif");
                        },
                        error:function (xhr, ajaxOptions, thrownError){
                            alert(xhr.statusText);
                        }
                    });
                } else {
                    $('#widget-list').addClass('red-border');
                }
            });
            $('#widget-list').change(function(){
                $('#widget-list').removeClass('red-border');
            });
